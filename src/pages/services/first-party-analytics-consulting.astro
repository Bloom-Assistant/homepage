---
import Layout from '~/layouts/PageLayout.astro';

import Header from '~/components/widgets/Header.astro';
import Hero2 from '~/components/widgets/Hero2.astro';
import Features from '~/components/widgets/Features.astro';
import Steps2 from '~/components/widgets/Steps2.astro';
import Content from '~/components/widgets/Content.astro';
import Prices from '~/components/widgets/Pricing.astro';

import { headerData } from '~/navigation';
import FAQs from '~/components/widgets/FAQs.astro';
import BlogLatestPosts from '~/components/widgets/BlogLatestPosts.astro';
import CallToAction from '~/components/widgets/CallToAction.astro';

const metadata = {
  title: 'First-party Analytics Consulting',
};
---

<Layout metadata={metadata}>
  <Fragment slot="header">
    <Header
      {...headerData}
      id="analytics-consulting-header"
      actions={[
        {
          variant: 'secondary',
          text: 'Schedule a free consultation',
          href: 'https://calendly.com/getoasis',
        },
      ]}
    />
  </Fragment>

  <!-- Hero2 Widget ******************* -->

  <Hero2
    tagline="First-party Analytics Consulting"
    actions={[
      { variant: 'primary', id:'analytics-consulting-hero', target: '_black', text: 'Retake your tagging', href: '#services' },
    ]}
    image={{
      src: 'https://kinsta.com/wp-content/uploads/2022/08/Cookieless-future.png',
      alt: 'Oasis Hero Image',
    }}
  >
    <Fragment slot="title">
        Thrive in a cookieless world with first-party analytics
    </Fragment>

    <Fragment slot="subtitle">
      <span class="hidden sm:inline">
        The new era of privacy is here, and with it comes a new era of tracking. With first-party analytics, you can take back control of your data and get the insights you need to grow your business.
    </Fragment>
  </Hero2>

  <!-- Features Widget *************** -->

  <Features
    id="services"
    title="What we can provide"
    subtitle="We can help you with a wide range of services to help you implement sGTM. Here are a few of the things we can help you with."
    columns={2}
    items={[
      {
        title: 'sGTM Consulting',
        description:
          'We can help you understand how Server Side Google Tag Manager can help your business and how to implement it.',
        icon: 'tabler:brand-google',
      },
      {
        title: 'CDP Implementation',
        description: `We can help you implement a CDP that will help you track the data you need to grow your business.`,
        icon: 'tabler:database',
      },
      {
        title: 'Custom Event Tracking',
        description:
          'We can help you track custom events that are important to your business.',
        icon: 'tabler:urgent',
      },
      {
        title: 'GDPR Compliance',
        description: `We can help you ensure that your tracking is GDPR compliant.`,
        icon: 'tabler:lock',
      },
    ]}
  />

  <Prices
  prices={[
    {
      title: 'sGTM Consulting',
      subtitle: 'Get a roadmap to sGTM success',
      price: ["$2500"],
      period: 'Flat Rate',
      items: [
        {
          description: 'Dedicated client portal',
        },
        {
          description: 'Get a 6 month roadmap for sGTM success',
        },
        {
          description: 'Exclusive discounts from our partners',
        },
        {
          description: 'Dedicated account manager',
        },
      ],
      callToAction: {
        id: 'sgtm-pricing-cta',
        text: 'Schedule a call',
        href: 'https://calendly.com/getoasis',
      },
    },
    {
      title: 'CDP Implementation',
      subtitle: 'Get a CDP that churns out insights',
      price: ["$150/hr"],
      period: 'Hourly Rate | Project Based',
      items: [
        {
          description: 'Everything from the audit package', 
        },
        {
          description: 'White glove implemetation',
        },
        {
          description: '6 months of maintenance',
        },
        {
          description: 'Dedicated account manager',
        },
      ],
      callToAction: {
        id: 'cdp-pricing-cta',
        text: 'Schedule a call',
        href: 'https://calendly.com/getoasis',
      },
      hasRibbon: true,
      ribbonTitle: 'popular',
    },
    {
      title: 'Custom Work',
      subtitle: 'We do custom work too!',
      price: ["Contact Us"],
      period: 'Custom Pricing | Installment Plan Available',
      items: [
        {
          description: 'Anything you need, we can do it!', 
        },
        {
          description: 'We can help you build a custom plan for your business',
        },
        {
          description: 'Rocket your business to the next level',
        },
        {
          description: 'You get a dedicated account manager',
        },
      ],
      callToAction: {
        id: 'custom-work-cta',
        text: 'Schedule a call',
        href: 'https://calendly.com/getoasis',
      },
    },
  ]}
/>


