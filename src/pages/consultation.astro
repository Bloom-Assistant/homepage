---
import Layout from '~/layouts/LandingLayout.astro';

import Hero from '~/components/widgets/Hero.astro';
import CallToAction from '~/components/widgets/CallToAction.astro';

const metadata = {
  title: 'Schedule a free consultation',
};
---

<Layout metadata={metadata}>
  <!-- Hero2 Widget ******************* -->

  <Hero
    tagline="Learn more about our services"
    title="Schedule a free consultation"
    subtitle="We are here to help you with your data and general technical implementations. Schedule a free consultation with us and we will help you to get started."
  />
    <!-- Calendly inline widget begin -->
<!-- Cal inline embed code begins -->
<div style="width:100%;height:100%;overflow:scroll" id="my-cal-inline"></div>
<script type="text/javascript">
  (function (C, A, L) { let p = function (a, ar) { a.q.push(ar); }; let d = C.document; C.Cal = C.Cal || function () { let cal = C.Cal; let ar = arguments; if (!cal.loaded) { cal.ns = {}; cal.q = cal.q || []; d.head.appendChild(d.createElement("script")).src = A; cal.loaded = true; } if (ar[0] === L) { const api = function () { p(api, arguments); }; const namespace = ar[1]; api.q = api.q || []; typeof namespace === "string" ? (cal.ns[namespace] = api) && p(api, ar) : p(cal, ar); return; } p(cal, ar); }; })(window, "https://app.cal.com/embed/embed.js", "init");
Cal("init",  {origin:"https://cal.com"});

  Cal("inline", {
	elementOrSelector:"#my-cal-inline",
	calLink: "getoasis/30min",
	layout: "month_view"
  });

  Cal("ui", {"styles":{"branding":{"brandColor":"#FC4C69"}},"hideEventTypeDetails":false,"layout":"month_view"});
  </script>
  <!-- Cal inline embed code ends -->

  <CallToAction
    title="Connect with us"
    subtitle="We are always available to answer any questions you may have. Contact us and we will get back to you as soon as possible."
    actions={[
      {
        variant: 'primary',
        text: 'Chat with us',
        href: 'https://www.servicebell.com/jdbohrman',
        icon: 'tabler:calendar-event',
      },
    ]}
  />
</Layout>
