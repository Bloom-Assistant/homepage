				<script>
				    export let Aria = document.getElementById('hamburger-button');

					function menuVisible(Aria) {
						if (Aria.getAttribute('aria-expanded') === 'false') {
							Aria.setAttribute('aria-expanded', 'true');
						} else {
							Aria.setAttribute('aria-expanded', 'false');
						}
					}
				</script>
				<!--Hamburger button-->
				<button id="hamburger-button" class="btn-md p-0 bg-transparent gray-800 dark-gray-200 xs-d-flex md-d-none"
								aria-label="mobile-menu"
								aria-expanded="false"
								onclick="menuVisible()">
					<svg aria-label="hamburger menu"
							 xmlns="http://www.w3.org/2000/svg"
							 width="26"
							 height="26"
